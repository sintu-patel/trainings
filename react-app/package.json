{
  "name": "react-namaskar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "test": "jest --bail --verbose",
    "test:watch": "jest --bail --watch --verbose",
    "test:updatesnap": "jest --updateSnapshot"
  },
  "keywords": [
    "react"
  ],
  "author": "Sintu Singh",
  "license": "",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "jest": "^22.4.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^16.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "webpack": "^2.7.0"
  },
  "jest": {
    "coverageDirectory": "./public",
    "testRegex": "\\.?(test|spec)\\.jsx?$",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/webpack/",
      "/node_modules/",
      "/server/",
      "/coverage/"
    ]
  }
}
